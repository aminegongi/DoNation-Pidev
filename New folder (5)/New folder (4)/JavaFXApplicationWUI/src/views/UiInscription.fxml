<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="592.0" prefWidth="407.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UiInscriptionController">
    <stylesheets>
        <URL value="@/styles/uiinscription.css" />
    </stylesheets>
   <children>
      <StackPane fx:id="stackInscri" prefHeight="592.0" prefWidth="407.0">
         <children>
            <ScrollPane prefHeight="581.0" prefWidth="455.0">
               <content>
                  <VBox fx:id="Flowpane" prefHeight="589.0" prefWidth="392.0" spacing="6.0" styleClass="scroll-bar" stylesheets="@../styles/uiinscription.css">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="47.0" prefWidth="371.0">
                           <children>
                              <ImageView fx:id="imgback" fitHeight="23.0" fitWidth="22.0" onMouseClicked="#BackToLogin" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/Icon/icons8_back_26px_1.png" />
                                 </image>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </ImageView>
                              <Separator orientation="VERTICAL" prefHeight="45.0" prefWidth="21.0" visible="false" />
                              <Label prefHeight="41.0" prefWidth="293.0" text="Inscription">
                                 <font>
                                    <Font name="Calibri" size="32.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                        <Separator prefHeight="19.0" prefWidth="371.0" visible="false" />
                        <JFXComboBox fx:id="CBType" onAction="#choixType" prefHeight="25.0" prefWidth="238.0" promptText=" Votre Type : " />
                        <Separator prefHeight="25.0" prefWidth="371.0" visible="false" />
                        <VBox fx:id="VBoxInscri" prefHeight="413.0" prefWidth="322.0" />
                        <Separator prefHeight="4.0" prefWidth="322.0" visible="false" />
                        <JFXButton fx:id="btInscri" onAction="#Inscri" prefHeight="18.0" prefWidth="67.0" stylesheets="@../styles/uiinscription.css" text="S'inscrire" />
                     </children>
                     <padding>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
